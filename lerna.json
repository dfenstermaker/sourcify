{
  "command": {
    "publish": {
      "ignoreChanges": ["*.md"],
      "message": "chore(release): publish",
      "registry": "https://npm.pkg.github.com"
    },
    "bootstrap": {
      "npmClientArgs": ["--no-package-lock"]
    },
    "version": {
      "allowBranch": "master",
      "message": "bump versions"
    }
  },
  "packages": [".", "ui", "packages/*"],
  "version": "independent"
}
